  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:dev": "playwright test --config=playwright.config.dev.ts",
    "test:qa": "playwright test --config=playwright.config.qa.ts",
    "test:stage": "playwright test --config=playwright.config.stage.ts",
    "test:uat": "playwright test --config=playwright.config.uat.ts",
    "test:prod": "playwright test --config=playwright.config.prod.ts",
    "lint": "eslint --ext .ts,.js . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --ext .ts,.js . --fix",
    "lint:report": "eslint --ext .ts,.js -f html -o eslint-report/index.html || true",
    "lint:report:json": "eslint --ext .ts,.js -f json -o eslint-report/eslint-results.json || true",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
